<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoggedInServlet</servlet-name>
    <servlet-class>LoggedInServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoggedInServlet</servlet-name>
    <url-pattern>/loggedin</url-pattern>
  </servlet-mapping>
 
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  
  
  
  
  <servlet>
    <servlet-name>InsertionServlet</servlet-name>
    <servlet-class>InsertServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InsertedServlet</servlet-name>
    <servlet-class>InsertedServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>InsertionServlet</servlet-name>
    <url-pattern>/save</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InsertedServlet</servlet-name>
    <url-pattern>/saved</url-pattern>
  </servlet-mapping>
 
  <welcome-file-list>
    <welcome-file>insert.html</welcome-file>
  </welcome-file-list>
  
  
  
  
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisteredServlet</servlet-name>
    <servlet-class>RegisteredServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisteredServlet</servlet-name>
    <url-pattern>/registered</url-pattern>
  </servlet-mapping>
 
  <welcome-file-list>
    <welcome-file>register.html</welcome-file>
  </welcome-file-list>
  
  
  
  
    <servlet>
    <servlet-name>RemoveServlet</servlet-name>
    <servlet-class>RemoveServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RemovedServlet</servlet-name>
    <servlet-class>RemovedServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>RemoveServlet</servlet-name>
    <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RemovedServlet</servlet-name>
    <url-pattern>/deleted</url-pattern>
  </servlet-mapping>
 
  <welcome-file-list>
    <welcome-file>remove.html</welcome-file>
  </welcome-file-list>
  
  
  
  
      <servlet>
    <servlet-name>SearchServlet</servlet-name>
    <servlet-class>SearchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchedServlet</servlet-name>
    <servlet-class>SearchedServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchedServlet</servlet-name>
    <url-pattern>/searched</url-pattern>
  </servlet-mapping>
 
  <welcome-file-list>
    <welcome-file>search.html</welcome-file>
  </welcome-file-list>
</web-app>